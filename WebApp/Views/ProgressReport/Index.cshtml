@model IEnumerable<WebApp.Data.ProgressReport>

@{
    ViewBag.Title = "Index";
}
<br />
<br />
@using (Html.BeginForm("Index", "ProgressReport",FormMethod.Get))
{
    <p>
        Student_Number: @Html.TextBox("stud")
    </p>
    <input type="submit" value="submit" />
 }

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.StudentID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ClassGroupID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Subject_Code)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Term1)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Term2)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Term3)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Term4)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.StudentID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ClassGroupID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Subject_Code)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Term1)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Term2)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Term3)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Term4)
        </td>
       
    </tr>
}

</table>
