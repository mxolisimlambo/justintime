@model IEnumerable<WebApp.Models.StreamTableViewModel>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        model.Stream_Code
        <th>
            @Html.DisplayNameFor(model => model.Stream_Code)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Description)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Stream_Code)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Description)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Stream_Code }) |
            @Html.ActionLink("Details", "Details", new { id=item.Stream_Code }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Stream_Code })
        </td>
    </tr>
}

</table>
