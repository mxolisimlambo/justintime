@model IEnumerable<WebApp.Models.StudentMarkViewModel>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
       
        <th>
            @Html.DisplayNameFor(model => model.Student_Number)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Student_Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ClassGroupe_id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.GroupeName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Subject_Code)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.mark)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.result)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Student_Number)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Student_Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ClassGroupe_id)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.GroupeName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Subject_Code)
        </td>
        <td>
            @Html.EditorFor(model => item.mark, new { htmlAttributes = new { @class = "form-control" } })
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.result)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Student_Number }) |
            @Html.ActionLink("Details", "Details", new { id=item.Student_Number }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Student_Number })
        </td>
    </tr>
}

</table>
