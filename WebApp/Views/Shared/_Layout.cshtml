                </div>
            </div>
        </div>*@

    <!-- Navigation -->
    <nav class="navbar navbar-default navbar-custom navbar-fixed-top">
        <div class="container-fluid">
            <!-- Brand and toggle get grouped for better mobile display -->
            <div class="navbar-header page-scroll">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                @Html.ActionLink("JG Zuma High Scool", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })
            </div>
            <!-- Collect the nav links, forms, and other content for toggling -->
            <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
                <ul class="nav navbar-nav navbar-right">
                    <li>@Html.ActionLink("Home", "Index", "Home")</li>
                    <li>@Html.ActionLink("About", "About", "Home")</li>
                    <li>@Html.ActionLink("Contact", "Contact", "Home")</li>
                    <li>@Html.ActionLink("School Managment", "Index", "SchoolManagment")</li>
                    <li>@Html.ActionLink("Student Portal", "Result", "SchoolManagment")</li>
                    <li>@Html.ActionLink("Click Here to Apply", "Create", "Application")</li>
                </ul>
            </div>
            <!-- /.navbar-collapse -->
        </div>
        <!-- /.container -->
    </nav>

    <!-- Page Header -->
    <!-- Set your background image for this header on the line below. -->@*'~/Content/template/img/home-bg.jpg'*@
    <header class="intro-header" style="background-image: url('@Url.Content("~/Content/template/img/home-bg.jpg")');width: 1600px;height: 1000px">
        <div class="container">
            <div class="row">
                <div class="col-lg-8 col-lg-offset-2 col-md-10 col-md-offset-1">
                    <div class="site-heading">
                        <h1>@ViewBag.Title</h1>
                        <hr class="small">
                        <span class="subheading">@ViewBag.Subheading</span>
                    </div>
                </div>
            </div>
        </div>
    </header>

    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - JustInTime Solutions</p>
        </footer>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    <script src="~/Scripts/jquery-ui.min.js"></script>
    <script>
    $(function() {
        $(".jqueryui-marker-datepicker").datepicker();
    });
    </script>

    <!-- Custom Theme JavaScript -->
    <script src="~/Content/template/js/clean-blog.min.js"></script>
    @RenderSection("scripts", required: false)
</body>
</html>



























